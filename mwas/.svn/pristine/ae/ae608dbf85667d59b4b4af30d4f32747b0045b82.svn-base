<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>mywebproject</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<!-- <servlet> <servlet-name>Home</servlet-name> <jsp-file>/pages/home.jsp</jsp-file> 
		</servlet> <servlet-mapping> <servlet-name>Home</servlet-name> <url-pattern>/mywebapp/home</url-pattern> 
		</servlet-mapping> -->
	<servlet>
		<servlet-name>MWAS</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>MWAS</servlet-name>
		<url-pattern>*.htm</url-pattern>
	</servlet-mapping>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
          /WEB-INF/MWAS-security.xml
          /WEB-INF/MWAS-servlet.xml
      </param-value>
	</context-param>
	<servlet>
		<servlet-name>WorkBookServlet</servlet-name>
		<servlet-class>com.datatable.WorkBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WorkBookServlet</servlet-name>
		<url-pattern>/WorkBookServlet</url-pattern>
	</servlet-mapping>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/pages/error.jsp</location>
	</error-page>
</web-app>